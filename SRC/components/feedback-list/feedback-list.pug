include ../feedback/feedback

mixin feedback-list({caption="Caption", count, feedbacks=[{caption: "Caption", text: "Error: items is not defined"}]}={})
  article.feedback-list
    header.feedback-list__caption
      h2.feedback-list__text #{caption}
      span.feedback-list__feedback-count #{count}
    ul.feedback-list__list
      for feedback in feedbacks
        li.feedback-list__item
          +feedback(feedback)