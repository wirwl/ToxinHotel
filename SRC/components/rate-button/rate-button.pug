mixin rate-button({disabled, id, count})  
  .rate-button(class=(disabled && "rate-button_disabled"))
    - let n=1;
    - const total=6;
    while n<total
      input(id=id+(total-n) type="radio" name=id checked=(count==total-n && "checked") ).rate-button__input
      label(for=id+(total-n)).rate-button__label.material-icons     
      - n++;