mixin form-elements({textInput, textInputFocused, guestsInputDropdown, maskedInput, datepickerInputArrival, datepickerInputCheckout, datepickerInputArrivalAndCheckout, subscribeInput, comfortInput, comfortInputExpanded, expandableCheckboxList, expandableCheckboxListExpanded, comfortStatusList, checkboxesList, radiosList, togglesList, likeButtonCaption, likeButton, likeButtonChecked, rateButtonCaption, rateButton4, rateButton5, guestsInput, checkboxesListRich, rangeSlider, buttonsCaption, buttonFilled, buttonFilledDisabled, buttonBordered, buttonBorderedDisabled, buttonSimple, buttonSimpleGray, buttonArrow, paginationCaption, pagination, guestsInputOpen, bulletsListCaption, bulletsList, feedback })
  .form-elements
    .form-elements__column1
      ul.form-elements__column1-list
        li.form-elements__column1-item
          +text-input(textInput)
        li.form-elements__column1-item
          +text-input(textInputFocused)
        li.form-elements__column1-item.form-elements__column1-wrapper-for-guests-input0
          +guests-input(guestsInputDropdown)
        li.form-elements__column1-item.form-elements__column1-wrapper-for-masked-input
          +masked-input(maskedInput)
        li.form-elements__column1-item.form-elements__column1-wrapper-for-datepicker-inputs
          ul.form-elements__column1-datepicker-inputs-list
            li.form-elements__column1-datepicker-inputs-item
              +datepicker-input(datepickerInputArrival)
            li.form-elements__column1-datepicker-inputs-item.form-elements__column1-datepicker-input-checkout
              +datepicker-input(datepickerInputCheckout)
        li.form-elements__column1-item.form-elements__column1-wrapper-for-fdd-input          
          +datepicker-input(datepickerInputArrivalAndCheckout)
        li.form-elements__column1-item.form-elements__column1-wrapper-for-subscribe-input
          +subscribe-input(subscribeInput)
        li.form-elements__column1-item
          .form-elements__column1-comfort-input
            +comfort-input(comfortInput)
        li.form-elements__column1-item.form-elements__column1-wrapper-for-comfort-input
          .form-elements__column1-comfort-input
            +comfort-input(comfortInputExpanded)
        li.form-elements__column1-item.form-elements__column1-wrapper-for-ecl1
          +expandable-checkbox-list(expandableCheckboxList)
        li.form-elements__column1-item.form-elements__column1-wrapper-for-ecl2                                                            
          +expandable-checkbox-list(expandableCheckboxListExpanded)
        li.form-elements__column1-item.form-elements__column1-wrapper-for-comfort-status-list
          +comfort-status-list(comfortStatusList)
    .form-elements__column-wrapper            
      .form-elements__column23-wrapper
        .form-elements__column2
          ul.form-elements__column2-list
            li.form-elements__column2-item.form-elements__column2-wrapper-for-checkboxes-list
              +checkboxes-list(checkboxesList)               
            li.form-elements__column2-item.form-elements__column2-wrapper-for-radios-list
              +radios-list(radiosList)
            li.form-elements__column2-item.form-elements__column2-wrapper-for-toggles-list 
              +toggles-list(togglesList)         
            li.form-elements__column2-item.form-elements__column2-wrapper-for-likes-button-list
              .form-elements__likes-buttons
                h3.form-elements__likes-button-list-caption #{likeButtonCaption}
                ul.form-elements__likes-button-list
                  li.form-elements__likes-button-list-item
                    +like-button(likeButton)
                  li.form-elements__likes-button-list-item
                    +like-button(likeButtonChecked)
            li.form-elements__column2-item.form-elements__column2-wrapper-for-rate-buttons-list
              .form-elements__rate-buttons
                h3.form-elements__rate-buttons-list-caption #{rateButtonCaption}
                ul.form-elements__rate-buttons-list
                  li.form-elements__rate-buttons-list-item
                    +rate-button(rateButton4)
                  li.form-elements__rate-buttons-list-item
                    +rate-button(rateButton5)
            li.form-elements__column2-item.form-elements__column2-wrapper-for-guests-input1
              +guests-input(guestsInput)
            li.form-elements__column2-item.form-elements__column2-wrapper-for-rich-checkboxes-list
              +checkboxes-list(checkboxesListRich)                                                 
        .form-elements__column3
          ul.form-elements__column3-list
            li.form-elements__column3-item.form-elements__column3-wrapper-for-range-slider
              +range-slider(rangeSlider)
            li.form-elements__column3-item.form-elements__column3-wrapper-for-buttons-list
              h3.form-elements__buttons-list-caption #{buttonsCaption}
              ul.form-elements__buttons-list    
                li.form-elements__buttons-list-item
                  ul.form-elements__two-buttons-list
                    li.form-elements__two-buttons-list-item.form-elements__two-buttons-list-item_width
                      +button-filled(buttonFilled)
                    li.form-elements__two-buttons-list-item.form-elements__two-buttons-list-item_width
                      +button-filled(buttonFilledDisabled)
                li.form-elements__buttons-list-item
                  ul.form-elements__two-buttons-list
                    li.form-elements__two-buttons-list-item.form-elements__two-buttons-list-item_width
                      +button-bordered(buttonBordered)
                    li.form-elements__two-buttons-list-item.form-elements__two-buttons-list-item_width
                      +button-bordered(buttonBorderedDisabled)
                li.form-elements__buttons-list-item
                  ul.form-elements__two-buttons-list.form-elements__two-buttons-list3
                    li.form-elements__two-buttons-list-item
                      +button-simple(buttonSimple)
                    li.form-elements__two-buttons-list-item
                      +button-simple(buttonSimpleGray)
                li.form-elements__buttons-list-item
                  +button-arrow(buttonArrow)
            li.form-elements__column3-item.form-elements__pagination-wrapper
              h3.form-elements__pagination-wrapper-caption #{paginationCaption}
              +pagination(pagination)
            li.form-elements__column3-item.form-elements__column3-wrapper-for-guests-input2
              +guests-input(guestsInputOpen)
            li.form-elements__column3-item.form-elements__column3-wrapper-for-bullet-list
              h3.form-elements__column3-bullet-list-caption #{bulletsListCaption}
              +bullets-list(bulletsList)
      .form-elements__row1
        +feedback(feedback)
