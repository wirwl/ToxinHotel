mixin index({mainTitle="Main title", items=[{title:"Error: items is not defined"}]}={})
  .index
    h1.index__title #{mainTitle}
    for item in items
      - const {title, path, hyperlinks=[{text: "Error: items is not defined"}]} = item
      h2.index__uikit #{title}
      ul.index__list
        for hyperlink in hyperlinks
          - const {link, text} = hyperlink
          li.index__list-item: a.index__list-item-link(href = path + link + ".html") #{text}